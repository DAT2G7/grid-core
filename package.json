{
    "name": "grid-core",
    "private": true,
    "license": "MIT",
    "type": "commonjs",
    "scripts": {
        "webpack": "webpack --mode=production",
        "__watch": "nodemon --watch src --exec \"ts-node\" src/index.ts",
        "_watch": "nodemon src/index.ts",
        "___watch": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/index.ts\"",
        "watch": "npx tsc && concurrently \"npx tsc -w\" \"nodemon watch/index.js\""
    },
    "devDependencies": {
        "concurrently": "^7.6.0",
        "nodemon": "^2.0.21",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.2",
        "webpack": "^5.76.2",
        "webpack-cli": "^5.0.1"
    }
}
