{
    "name": "grid-core",
    "private": true,
    "license": "MIT",
    "type": "commonjs",
    "scripts": {
        "webpack": "webpack --mode=production",
        "build": "npx tsc",
        "watch": "npx tsc && concurrently \"npx tsc -w\" \"nodemon watch/index.js\"",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "test": "jest --verbose"
    },
    "jest": {
        "preset": "ts-jest"
    },
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "concurrently": "^7.6.0",
        "nodemon": "^2.0.21",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.2",
        "webpack": "^5.76.2",
        "webpack-cli": "^5.0.1",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.7.0",
        "prettier": "^2.8.4"
    }
}
